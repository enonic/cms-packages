<theme>
    <region-prefix/>
    <accessibility>
        <access-key key="m" text="accessibility-text-main-menu" anchor="menu-main" />
        <access-key key="s" text="accessibility-text-sub-menu" anchor="menu-sub" />
        <access-key key="c" text="accessibility-text-center" anchor="center" />
    </accessibility>
    <device-classes>
        <device-class name="pc,unknown">
            <layout name="default">
                <area width="960">
                    <region name="west">
                        <width>344</width>
                        <margin>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>0</bottom>
                        </margin>
                        <padding>
                            <top>0</top>
                            <left>10</left>
                            <bottom>0</bottom>
                        </padding>
                    </region>
                </area>
            </layout>
            <layout name="section-layout">
                <area width="960">
                    <region name="north">
                        <width>960</width>
                        <padding>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>0</bottom>
                        </padding>
                        <margin>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>0</bottom>
                        </margin>
                    </region>
                </area>
                <area width="960">
                    <region name="west">
                        <width>344</width>
                        <margin>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>0</bottom>
                        </margin>
                        <padding>
                            <top>0</top>
                            <left>0</left>
                            <bottom>0</bottom>
                        </padding>
                    </region>
                </area>
            </layout>
            <layout name="spot-layout">
                <area width="960">
                    <region name="north">
                        <width>960</width>
                        <padding>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>0</bottom>
                        </padding>
                        <margin>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>0</bottom>
                        </margin>
                    </region>
                </area>
                <area width="960">
                    <region name="west">
                        <width>344</width>
                        <margin>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>0</bottom>
                        </margin>
                        <padding>
                            <top>0</top>
                            <left>0</left>
                            <bottom>0</bottom>
                        </padding>
                    </region>
                </area>
                <area width="960">
                    <region name="south">
                        <width>960</width>
                        <padding>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>0</bottom>
                        </padding>
                        <margin>
                            <right>0</right>
                            <left>0</left>
                            <top>0</top>
                            <bottom>30</bottom>
                        </margin>
                    </region>
                </area>
            </layout>
            <image>
                <filters>
                    <filter>rounded(4)</filter>
                </filters>
                <sizes>
                    <size name="full">
                        <filter>scalewidth</filter>
                        <width>1</width>
                    </size>
                    <size name="wide">
                        <filter>scalewide</filter>
                        <width>1</width>
                        <height>0.4</height>
                    </size>
                    <size name="regular">
                        <filter>scalewidth</filter>
                        <width>0.4</width>
                    </size>
                    <size name="list">
                        <filter>scalewidth</filter>
                        <width>0.3</width>
                    </size>
                    <size name="square">
                        <filter>scalesquare</filter>
                        <size>0.4</size>
                    </size>
                    <size name="thumbnail">
                        <filter>scalesquare</filter>
                        <size>0.1</size>
                    </size>
                </sizes>
            </image>                                     +
            <styles>
                <style>/_public/themes/bluman-travels/jquery/ui-custom-1.8.16/jquery-ui-1.8.16.custom.css</style>
                <style>/_public/themes/bluman-travels/jquery/tooltip-1.3.0/jquery.tooltip.css</style>
                <style>/_public/themes/bluman-travels/jquery/captify.tiny.css</style>
                <style>/_public/themes/bluman-travels/jquery/jquery-easing/jquery.easing.1.3.css</style>
                <style>/_public/themes/bluman-travels/jquery/jquery-selectBox/jquery.selectBox.css</style>
                <style>/_public/themes/bluman-travels/all.css</style>
                <style>/_public/themes/bluman-travels/pc.css</style>
                <style condition="lt IE 8">/_public/sites/advanced/ie.css</style>
                <style>/_public/themes/bluman-travels/all-bluman.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-footer.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-center.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-header.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-north.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-south.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-west.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-spot-search.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-spots-nearby.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-spot-thumbnails.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-spot-show.css</style>
                <style>/_public/themes/bluman-travels/bluman-pc-menus.css</style>
                <style>/_public/themes/bluman-travels/bluman-elements.css</style>
            </styles>
            <scripts>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery-1.6.2.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.validate.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery-ui-1.8.16.custom.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.tooltip.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.cookie.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.selectBox.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.bgiframe.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.dimensions.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.cycle.lite.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/captify.tiny.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.easing.1.3.js</script>
                <script>/_public/themes/bluman-travels/scripts/common-all.js</script>
                <script>/_public/themes/bluman-travels/scripts/common-pc.js</script>
            </scripts>
        </device-class>
        <device-class name="mobile">
            <layout name="default">
                <area width="320">
                    <regions>
                        <region name="north">
                            <width>296</width>
                            <padding>
                                <left>12</left>
                                <right>12</right>
                                <top>12</top>
                                <bottom>12</bottom>
                            </padding>
                        </region>
                        <region name="center">
                            <width>320</width>
                        </region>
                        <region name="south">
                            <width>296</width>
                            <padding>
                                <left>12</left>
                                <right>12</right>
                                <top>12</top>
                                <bottom>12</bottom>
                            </padding>
                        </region>
                    </regions>
                </area>                    
            </layout>
            <image>
                <filters>
                    <filter>rounded(4)</filter>
                </filters>
                <sizes>
                    <size name="full">
                        <filter>scalewidth</filter>
                        <width>1</width>
                    </size>
                    <size name="wide">
                        <filter>scalewide</filter>
                        <width>1</width>
                        <height>0.4</height>
                    </size>
                    <size name="regular">
                        <filter>scalewidth</filter>
                        <width>0.4</width>
                    </size>
                    <size name="list">
                        <filter>scalewidth</filter>
                        <width>0.3</width>
                    </size>
                    <size name="square">
                        <filter>scalesquare</filter>
                        <size>0.4</size>
                    </size>
                    <size name="thumbnail">
                        <filter>scalesquare</filter>
                        <size>0.1</size>
                    </size>
                </sizes>
            </image>
            <styles>
                <style>/_public/themes/bluman-travels/all.css</style>
                <style>/_public/themes/bluman-travels/mobile.css</style>
                <style>/_public/themes/bluman-travels/jquery/ui-custom-1.8.16/jquery-ui-1.8.16.custom.css</style>
                <style>/_public/themes/bluman-travels/jquery/tooltip-1.3.0/jquery.tooltip.css</style>
                <style>/_public/themes/bluman-travels/mobile/default/style.css</style>
                <style>/_public/themes/bluman-travels/mobile/bluman.css</style>
                <style>/_public/themes/bluman-travels/all-bluman.css</style>
            </styles>
            <scripts>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery-1.6.2.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.validate.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery-ui-1.8.16.custom.min.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.cookie.js</script>
                <script>/_public/libraries/jquery/scripts-1.6.2/jquery.enonic.tree.js</script>
                <script>/_public/themes/bluman-travels/scripts/common-all.js</script>
            </scripts>
        </device-class>
    </device-classes>
</theme>
